{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "declaration": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/graphqlOperations/types/fetchCalls.ts", "src/graphqlOperations/types/loginUser.js", "src/utils/AuthRoute.tsx", "src/authContext/auth.tsx", "src/ApolloProvider.jsx"]
}
